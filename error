Pattern pattern = Pattern.compile("(?i)" + Pattern.quote(label) + "\\s*[:\\-]?\\s*(.+)");
Matcher matcher = pattern.matcher(text);

if (matcher.find()) {
    String value = matcher.group(1).trim();
    labelExisted.put(label, value);
    log.info("Matched {}: {}", label, value);
    addToMetadataList(metadata, key, value, page);
} else {
    log.warn("No match found for label: {}", label);
}
