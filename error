public class FileContent {
    private String uploadBatch;
    private String uploadFileId;

    // Constructors, Getters and Setters
    public FileContent() {}
    public FileContent(String uploadBatch, String uploadFileId) {
        this.uploadBatch = uploadBatch;
        this.uploadFileId = uploadFileId;
    }
    public String getUploadBatch() {
        return uploadBatch;
    }
    public void setUploadBatch(String uploadBatch) {
        this.uploadBatch = uploadBatch;
    }
    public String getUploadFileId() {
        return uploadFileId;
    }
    public void setUploadFileId(String uploadFileId) {
        this.uploadFileId = uploadFileId;
    }
}



public class Properties {
    private String dcTitle;
    private FileContent fileContent;

    // Constructors, Getters and Setters
    public Properties() {}
    public Properties(String dcTitle, FileContent fileContent) {
        this.dcTitle = dcTitle;
        this.fileContent = fileContent;
    }

    public String getDcTitle() {
        return dcTitle;
    }

    public void setDcTitle(String dcTitle) {
        this.dcTitle = dcTitle;
    }

    public FileContent getFileContent() {
        return fileContent;
    }

    public void setFileContent(FileContent fileContent) {
        this.fileContent = fileContent;
    }
}




import com.fasterxml.jackson.annotation.JsonProperty;

public class DocumentRequest {
    @JsonProperty("entity-type")
    private String entityType;
    private String name;
    private String type;
    private Properties properties;

    // Constructors, Getters and Setters
    public DocumentRequest() {}

    public DocumentRequest(String entityType, String name, String type, Properties properties) {
        this.entityType = entityType;
        this.name = name;
        this.type = type;
        this.properties = properties;
    }

    public String getEntityType() {
        return entityType;
    }

    public void setEntityType(String entityType) {
        this.entityType = entityType;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public Properties getProperties() {
        return properties;
    }

    public void setProperties(Properties properties) {
        this.properties = properties;
    }
}

